    },
    "dependencies": {
        "@angular/core": "^8.2.7",
        "rxjs": "^6.5.3",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/schematics": "^9.0.6",
        "@angular/compiler": "^8.2.7",
        "@angular/compiler-cli": "^8.2.7",
        "ng-packagr": "^5.5.1",
        "tslib": "^1.10.0",
        "typescript": "^3.5.3"
    }
}
