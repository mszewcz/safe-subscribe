  "devDependencies": {
    "@angular-devkit/schematics": "^11.2.11",
    "@angular/compiler": "^11.2.12",
    "@angular/compiler-cli": "^11.2.12",
    "ng-packagr": "^11.2.4",
    "tslib": "^2.2.0",
    "typescript": "^4.1.5"
  }
